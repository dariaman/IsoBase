@model IsoBase.ViewModels.CreatePicCodeVM

@{
    ViewData["Title"] = "Create New Pic Code";
}

    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="PicDesc" class="control-label"></label>
                    <input asp-for="PicDesc" class="form-control" />
                    <span asp-validation-for="PicDesc" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Remark" class="control-label"></label>
                    <input asp-for="Remark" class="form-control" />
                    <span asp-validation-for="Remark" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>